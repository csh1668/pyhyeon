# 메서드 안에서 객체 생성 테스트

class Point:
  def __init__(self, x):
    self.x = x
  
  def make_copy(self):
    print("Making copy with x:")
    print(self.x)
    new_point = Point(self.x)
    print("Created new point")
    return new_point

p1 = Point(10)
print("p1.x:")
print(p1.x)

p2 = p1.make_copy()
print("p2.x:")
print(p2.x)

