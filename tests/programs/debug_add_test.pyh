# 디버그 테스트

class Number:
  def __init__(self, val):
    self.val = val
  
  def __add__(self, other):
    print("__add__ start")
    print(self.val)
    print(other.val)
    sum_val = self.val + other.val
    print("sum:")
    print(sum_val)
    new_obj = Number(sum_val)
    print("created object, returning")
    return new_obj

print("Test 1: regular add")
print(5 + 3)

print("Test 2: create objects")
a = Number(5)
print("a created")
b = Number(3)
print("b created")

print("Test 3: call __add__")
c = a + b
print("__add__ returned, c =")
print(c)

