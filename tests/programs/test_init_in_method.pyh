class Number:
  def __init__(self, val):
    print("init called with")
    print(val)
    self.val = val
  
  def create_another(self):
    print("In create_another")
    result = Number(99)
    print("After creating Number")
    print("result.val:")
    print(result.val)
    return result

n1 = Number(5)
print("n1 created")
n2 = n1.create_another()
print("n2 created")
print("n2.val:")
print(n2.val)


