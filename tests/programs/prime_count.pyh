def is_prime(n):
  if n < 2:
    return False
  i = 2
  while i * 2 <= n and i * i <= n:
    if n % i == 0:
      return False
    i = i + 1
  return True

def count_primes(limit):
  c = 0
  i = 2
  while i <= limit:
    if is_prime(i):
      c = c + 1
    i = i + 1
  return c

print(count_primes(20000))

