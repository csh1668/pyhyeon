# 연산자 매직 메서드 테스트

# 기본 산술 연산자
print(1 + 2)  # 3
print(10 - 3)  # 7
print(4 * 5)  # 20
print(15 // 3)  # 5
print(17 % 5)  # 2

# 단항 연산자
print(-10)  # -10
print(+10)  # 10

# String concatenation
print("Hello" + " " + "World")  # Hello World

# String repetition
print("A" * 3)  # AAA
print(2 * "B")  # BB

# 비교 연산자
print(5 < 10)  # True
print(10 > 5)  # True
print(5 <= 5)  # True
print(10 >= 10)  # True
print(5 == 5)  # True
print(5 != 10)  # True

# String 비교
print("apple" < "banana")  # True
print("zoo" > "ant")  # True
print("test" == "test")  # True
print("a" != "b")  # True


